# Production Environment Variables
# These override the defaults in .env for the production environment

# In production, we generally want relative URLs
# This allows the frontend to be served from any path
# and will make requests to /api on the same domain
VITE_API_URL=/api

# Shorter timeout for production
VITE_API_TIMEOUT=15000

# Disable development-only features
VITE_ENABLE_FEATURE_A=false
VITE_ENABLE_FEATURE_B=false